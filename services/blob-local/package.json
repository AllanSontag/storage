{
  "name": "@vercel/blob-local",
  "version": "0.0.1",
  "private": true,
  "description": "Server that emulates the Vercel Blob API for local development",
  "homepage": "https://vercel.com/storage/blob",
  "repository": {
    "type": "git",
    "url": "https://github.com/vercel/storage.git",
    "directory": "services/blob-local"
  },
  "license": "MIT",
  "scripts": {
    "dev": "ENV=dev wgo run main.go",
    "docker:build": "docker build -t zeit/vercel-blob-local .",
    "docker:run": "docker run -d -p 3001:3001 --name vercel-blob-local zeit/vercel-blob-local",
    "docker:start": "docker start vercel-blob-local",
    "docker:stop": "docker stop vercel-blob-local",
    "start": "ENV=dev go run main.go"
  }
}
